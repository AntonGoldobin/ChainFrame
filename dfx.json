{
  "canisters": {
    "backend": {
      "type": "rust",
      "candid": "src/backend/backend.did",
      "package": "backend"
    },
    "frontend": {
      "dependencies": ["backend"],
      "type": "assets",
      "frontend": {
        "entrypoint": "dist/index.html"
      },
      "source": ["dist/"]
    }
  },
  "defaults": {
    "build": {
      "packtool": "npm run --silent sources"
    }
  }
}
